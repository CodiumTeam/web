{
  "parser": "@babel/eslint-parser",
  "settings": {
    "react": {
        "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "extends": ["eslint:recommended", "prettier", "plugin:cypress/recommended", "plugin:react/recommended"],
  "globals": {
    "grecaptcha": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off"
  }
}
