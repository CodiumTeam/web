{
  "name": "web",
  "version": "1.0.0",
  "description": "Codium Web app",
  "main": "index.js",
  "engines": {
    "node": ">18.x"
  },
  "scripts": {
    "i18n:scan": "i18next-scanner --config i18next-scanner.config.js",
    "start": "vite --host 0.0.0.0 --port 3000",
    "build": "NODE_ENV=production vite build --mode=production",
    "postbuild": "NODE_ENV=production npm run i18n:scan && static-i18n -l es -i en -i es dist --replace true",
    "lint-staged": "lint-staged",
    "lint": "eslint **/*.js",
    "heroku-cleanup": "rm -rf node_modules",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodiumTeam/web.git"
  },
  "keywords": [],
  "author": "Codium Team <info@codium.team>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodiumTeam/web/issues"
  },
  "homepage": "https://github.com/CodiumTeam/web#readme",
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@testing-library/cypress": "^9.0.0",
    "@vitejs/plugin-react": "3.1.0",
    "cypress": "12.6.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "glob": "^8.1.0",
    "i18next-scanner": "^4.2.0",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "sass": "1.58.3",
    "static-i18n": "^0.2.10",
    "vite": "4.1.4",
    "vite-plugin-ejs-engine": "1.0.4"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@emailjs/browser": "^3.11.0",
    "@glidejs/glide": "3.6.0",
    "@stackblitz/sdk": "1.8.2",
    "isinview": "2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-modal": "3.16.1",
    "react-toastify": "9.1.1"
  }
}
